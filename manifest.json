{
  
  "manifest_version": 2,
  "name": "Translate++",
  "version": "0.1",
  "description": "Adds the functionality of translating to multiple languages at once",
  "permissions": [
	  "https://www.collinsdictionary.com/dictionary/german-english/*"
  ],

  "content_scripts": [
  {
    "matches": [
      "https://translate.google.com/*"
    ],
    "js": ["content.js"]
  }
  ]
}

